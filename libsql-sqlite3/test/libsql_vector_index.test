proc error_messages {sql} {
  set ret ""
  catch {
    set stmt [sqlite3_prepare db $sql -1 dummy]
    sqlite3_step $stmt
    sqlite3_finalize $stmt
  } ret
  set ret [sqlite3_errmsg db]
}

